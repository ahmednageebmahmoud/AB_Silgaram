<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white" ng-app="ngApp">

    <div class="page-wrapper" ng-controller="ctrlLayOut">
        <!-- BEGIN HEADER -->
        <div class="page-header navbar navbar-fixed-top">
            <!-- BEGIN HEADER INNER -->
            <div class="page-header-inner ">
                <!-- BEGIN LOGO -->
                <div class="page-logo">
                    <a href="/">
                        <img src="~/Files/Images/LoginPage/logo.png" alt="logo" class="logo-default" width="25" />
                    </a>
                    <div class="menu-toggler sidebar-toggler">
                        <span></span>
                    </div>
                </div>
                <!-- END LOGO -->
                <!-- BEGIN RESPONSIVE MENU TOGGLER -->
                <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
                    <span></span>
                </a>
                <!-- END RESPONSIVE MENU TOGGLER -->
                <!-- BEGIN TOP NAVIGATION MENU -->
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        <!-- BEGIN NOTIFICATION DROPDOWN -->
                        <!-- DOC: Apply "dropdown-dark" class after "dropdown-extended" to change the dropdown styte -->
                        <!-- DOC: Apply "dropdown-hoverable" class after below "dropdown" and remove data-toggle="dropdown" data-hover="dropdown" data-close-others="true" attributes to enable hover dropdown mode -->
                        <!-- DOC: Remove "dropdown-hoverable" and add data-toggle="dropdown" data-hover="dropdown" data-close-others="true" attributes to the below A element with dropdown-toggle class -->
                        <!--Notifications-->
                        <li class="dropdown dropdown-extended dropdown-notification" id="header_notification_bar">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <i class="icon-bell"></i>
                                <span class="badge badge-default">{{NotificationCount}}  </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="external">
                                    <h3>
                                        <span class="bold"> {{AllNotifications.length}} </span> @Token.Notifications @Token.From  {{NotificationCount}}
                                    </h3>
                                    <a href="~/UserProfile" ng-show="false">@Token.ViewAll</a>
                                </li>
                                <li>
                                    <ul class="dropdown-menu-list scroller" style="height: 250px;" data-handle-color="#637283">
                                        <li ng-repeat="x in AllNotifications">
                                            <a ng-href="{{x.RedirectUrl==null?'/Notifications?NotId='+x.Id:x.RedirectUrl}}">
                                                <div>
                                                    <span class="time">{{x.ActionDate}}</span>
                                                    <span class="details">
                                                        <span class="label label-sm label-icon " ng-class="x.CssClassStyle">
                                                            <i ng-class="x.CssClassIcon"></i>
                                                        </span>
                                                    </span>
                                                </div>
                                                <span>
                                                    {{x.Notify}}
                                                </span>
                                            </a>
                                        </li>
                                        <li>
                                            <center>

                                                <button type="button" class="btn btn-default mt-ladda-btn ladda-button btn-circle" data-style="expand-left" data-spinner-color="#333" ng-click="getNotifications(true)">
                                                    <span class="ladda-label">@Token.ShowMore</span>
                                                    <span class="ladda-spinner"></span>
                                                </button>
                                            </center>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                        <!-- END NOTIFICATION DROPDOWN -->
                        <!-- BEGIN INBOX DROPDOWN -->
                        <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
                        <li class="dropdown dropdown-extended dropdown-inbox" id="header_inbox_bar">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <i class="icon-envelope-open"></i>
                                <span class="badge badge-default"> {{TechnicalSupportCount}} </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="external">
                                    <h3>

                                        <span class="bold"></span>

                                        <span class="bold"> {{TechnicalSupport.length}} </span> @Token.TechnicalSupport @Token.From  {{TechnicalSupportCount}}
                                    </h3>

                                    <a href="app_inbox.html" ng-show="false">view all</a>
                                </li>
                                <li>
                                    <ul class="dropdown-menu-list scroller" style="height: 275px;" data-handle-color="#637283">
                                        <li ng-repeat="x in TechnicalSupport">
                                            <a href="{{x.RedirectUrl}}">
                                                @*<span class="photo">
                                                        <img src="../assets/layouts/layout3/img/avatar2.jpg" class="img-circle" alt="">
                                                    </span>*@
                                                <span class="subject">
                                                    <span class="from">{{x.UserName}} </span>
                                                    <span class="time">{{x.Date}} </span>
                                                </span>
                                                <span class="message">{{x.Message.substring(0,30)}}...</span>
                                            </a>
                                        </li>
                                        <li>
                                            <center>
                                                <button type="button" class="btn btn-default mt-ladda-btn ladda-button btn-circle" data-style="expand-left" data-spinner-color="#333" ng-click="getTechnicalSupport(true)">
                                                    <span class="ladda-label">@Token.ShowMore</span>
                                                    <span class="ladda-spinner"></span>
                                                </button>
                                            </center>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-user">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <img alt="" class="img-circle" src="{{UserData.ImageUrl.replace('~','')}}" />
                                <span class="username username-hide-on-mobile"> {{UserData.UserName}} </span>
                                <i class="fa fa-angle-down"></i>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-default">
                                <li>
                                    <a href="~/UserProfile">
                                        <i class="icon-user"></i> @Token.UserProfile
                                    </a>
                                </li>
                                <li ng-show="false">
                                    <a href="app_calendar.html">
                                        <i class="icon-calendar"></i> My Calendar
                                    </a>
                                </li>
                                <li>
                                    <a href="app_inbox.html" ng-show="false">
                                        <i class="icon-envelope-open"></i> My Inbox
                                        <span class="badge badge-danger"> 3 </span>
                                    </a>
                                </li>
                                <li>
                                    <a href="app_todo.html" ng-show="false">
                                        <i class="icon-rocket"></i> My Tasks
                                        <span class="badge badge-success"> 7 </span>
                                    </a>
                                </li>
                                <li class="divider"> </li>
                                <li>
                                    <a href="page_user_lock_1.html" ng-show="false">
                                        <i class="icon-lock"></i> Lock Screen
                                    </a>
                                </li>
                                <li>
                                    <a href="~/Logout">
                                        <i class="icon-key"></i> @Token.Logout
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <!-- END USER LOGIN DROPDOWN -->
                        <!-- BEGIN QUICK SIDEBAR TOGGLER -->
                        <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
                        <li class="dropdown dropdown-quick-sidebar-toggler" ng-show="false">
                            <a href="javascript:;" class="dropdown-toggle">
                                <i class="icon-logout"></i>
                            </a>
                        </li>
                        <!-- END QUICK SIDEBAR TOGGLER -->
                    </ul>
                </div>
                <!-- END TOP NAVIGATION MENU -->
            </div>
            <!-- END HEADER INNER -->
        </div>
        <!-- END HEADER -->
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN SIDEBAR -->
            @Html.Partial("_Menu")
            <!-- END SIDEBAR -->
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <div class="page-content">
                    <div class="page-bar">
                        <ul class="page-breadcrumb">
                            <li>
                                <a href="Home">@Token.Home</a>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span> @ViewBag.Title</span>
                            </li>
                        </ul>
                        <div class="page-toolbar">
                            <div id="dashboard-report-range" class="pull-right tooltips btn btn-sm" data-container="body" data-placement="bottom" data-original-title="Change dashboard date range">
                                <i class="icon-calendar"></i>&nbsp;
                                <span class="thin uppercase hidden-xs"></span>&nbsp;
                                <i class="fa fa-angle-down"></i>
                            </div>
                        </div>
                    </div>
                    <!-- END PAGE BAR -->
                    <!-- BEGIN PAGE TITLE-->
                    <h1 class="page-title">
                        @ViewBag.Title
                        <small> @ViewBag.Description</small>
                    </h1>
                    <!-- END PAGE TITLE-->
                    <!-- END PAGE HEADER-->
                    @RenderBody()
                </div>
                <!-- END CONTENT BODY -->
            </div>
            <!-- END QUICK SIDEBAR -->
        </div>
        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <div class="page-footer">
            <div class="page-footer-inner">
                @DateTime.Now.Year &copy; @Token.AppName
            </div>
            <div class="scroll-to-top">
                <i class="icon-arrow-up"></i>
            </div>
        </div>
        <!-- END FOOTER -->
    </div>
    <!-- BEGIN QUICK NAV -->

    <div class="quick-nav-overlay"></div>
    <!-- END QUICK NAV -->
